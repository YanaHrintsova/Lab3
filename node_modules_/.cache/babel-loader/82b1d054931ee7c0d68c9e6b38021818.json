ourcesContent":["<template>\r\n  <div class=\"EconomyText\">\r\n    <div class=\"btn\">\r\n      <button @click=\"show=!show\" class=\"line\">Створити нове меню</button>\r\n    </div>\r\n    \r\n    <div v-show=\"show\">\r\n      <AddPolits @add-politics=\"addTodo\" :postPolits=\"postPolits\" />\r\n    </div>\r\n    \r\n    <Loader v-if=\"loading\" />\r\n    <PolitsList v-else-if=\"politics.length\" v-bind:politics=\"politics\"  v-bind:postPolits=\"postPolits\" @remove-blog=\"removeComment\"/>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PolitsList from \"../NewText/PolitsList\";\r\nimport AddPolits from \"../NewText/AddPolits\";\r\nimport Loader from \"../Loader/Loader\";\r\nexport default {\r\n  name: \"EconomyText\",\r\n  data() {\r\n    return {\r\n      politics: [],\r\n      loading: true,\r\n      postPolits: \"basic\",\r\n      show:false\r\n    };\r\n  },\r\n  mounted() {\r\n    fetch(\"http://localhost:4000/app/getbasic\")\r\n      .then((response) => response.json())\r\n      .then((json) => {\r\n        this.politics = json.polits;\r\n        console.log(json.polits);\r\n        this.loading = false;\r\n      });\r\n  },\r\n  components: {\r\n    PolitsList,\r\n    AddPolits,\r\n    Loader,\r\n  },\r\n  methods: {\r\n    addTodo(polits) {\r\n      console.log(polits)\r\n      this.politics.push(polits);\r\n    },\r\n    removeComment(_id){\r\n      this.politics = this.politics.filter(t=>t._id !==_id)\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\ntextarea {\r\n  width: 100%;\r\n  height: 100px;\r\n}\r\n.line{\r\n  font-size: 28px;\r\n  border-radius: 15px;\r\n  margin-left: 50%;\r\n}\r\n.btn{\r\n  width: 100%;\r\n}\r\n</style>"],"sourceRoot":"src/views/Basic"}]}